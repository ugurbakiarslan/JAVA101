package Calisma;

import java.lang.runtime.SwitchBootstraps;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		String tercih ;
		
		Switch(tercih){
			case a:
				System.out.println("Girilen değer a");
				break;
			case 1:
				System.out.println("Girilen değer 1");
				break;
		}
		
		
	}

}
